	doi = {10.3390/polym4010275},
	file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Huang, Terentjev/Polymers/Huang, Terentjev - 2012 - Dispersion of carbon nanotubes Mixing, sonication, stabilization, and composite properties.pdf:pdf},
